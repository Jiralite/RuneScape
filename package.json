{
	"name": "runescape",
	"version": "0.13.1",
	"description": "A library to interact with the non-existent RuneScape API.",
	"keywords": [
		"RuneScape",
		"Jagex",
		"RS3"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Jiralite/RuneScape.git"
	},
	"license": "MIT",
	"author": "Jiralite",
	"type": "module",
	"exports": "./distribution/index.js",
	"types": "./distribution/index.d.ts",
	"files": [
		"distribution"
	],
	"scripts": {
		"test": "tsc --noEmit && vitest run",
		"build": "tsup",
		"lint": "biome check",
		"format": "biome check --write",
		"prepack": "pnpm run build && pnpm run lint",
		"update": "pnpm update --interactive --latest && pre-commit autoupdate"
	},
	"dependencies": {
		"csv-parse": "^6.1.0",
		"dayjs": "^1.11.19"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.3.7",
		"@types/node": "^24.10.1",
		"tsup": "^8.5.1",
		"typescript": "^5.9.3",
		"vitest": "^4.0.13"
	},
	"devEngines": {
		"runtime": {
			"name": "node",
			"version": ">=24",
			"onFail": "error"
		},
		"packageManager": {
			"name": "pnpm",
			"version": ">=10.23.0",
			"onFail": "error"
		}
	}
}
