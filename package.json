{
	"name": "runescape",
	"version": "0.10.0",
	"description": "A library to interact with the non-existent RuneScape API.",
	"keywords": ["RuneScape", "Jagex", "RS3"],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Jiralite/RuneScape.git"
	},
	"license": "MIT",
	"author": "Jiralite",
	"type": "module",
	"exports": "./distribution/index.js",
	"types": "./distribution/index.d.ts",
	"files": ["distribution"],
	"scripts": {
		"build": "tsup",
		"test": "tsc --noEmit",
		"lint": "biome check",
		"format": "biome check --write",
		"prepack": "pnpm run build && pnpm run lint",
		"update": "pnpm update --interactive --latest && pre-commit autoupdate"
	},
	"dependencies": {
		"dayjs": "^1.11.10"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.9.4",
		"@types/node": "^22.9.1",
		"tsup": "^8.3.5",
		"typescript": "^5.3.3"
	},
	"devEngines": {
		"runtime": {
			"name": "node",
			"version": ">=20",
			"onFail": "error"
		},
		"packageManager": {
			"name": "pnpm",
			"version": ">=9.14.2",
			"onFail": "error"
		}
	}
}
