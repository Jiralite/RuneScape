	},
	"devDependencies": {
		"@types/node": "^20.11.0",
		"eslint": "^8.56.0",
		"eslint-config-neon": "^0.1.57",
		"prettier": "^3.2.2",
		"tsup": "^8.0.1",
		"typescript": "^5.3.3"
	},
	"packageManager": "pnpm@8.14.1"
}
